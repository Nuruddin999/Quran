{
  "verse": {
    "1": "Скажи: «Шукаю захисту в Господа людей,",
    "2": "Володаря людей,",
    "3": "Бога людей,",
    "4": "від зла спокусника зникаючого,",
    "5": "який зваблює серця людей,",
    "6": "від зла джинів і людей»."
  },
  "count": 6,
  "name": "Ан-Нас",
  "translation": "Люди",
  "index": 114
}